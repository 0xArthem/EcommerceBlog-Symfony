<form method="post" action="{{ path('app_address_delete', {'id': address.id}) }}" onsubmit="return confirm('Confirmez-vous vouloir supprimer cette adresse ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ address.id) }}">
	<button class="btn btn-voir-fiche-produit bg-dark">Supprimer</button>
</form>
